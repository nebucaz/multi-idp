{
  "name": "ts-node",
  "version": "1.0.0",
  "description": "Node with typescript",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "scripts": {
    "start-ts": "ts-node src/server.ts",
    "start": "tsc --resolveJsonModule && node dist/server.js",
    "lint": "eslint . --ext .ts --ext .mts",
    "dev-does-not-work": "ts-node-dev dist/server.js",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/server.ts\""

  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "openid-client": "^5.1.6"
  }
}
